{
    "name": "hafael/easyrec-laravel",
    "description": "Laravel 5.3+ integration for the Easyrec package.",
    "keywords": [
        "php",
        "hafael",
        "laravel",
        "easyrec"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "VerdeIT",
            "email": "villa655321verde@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": ">=5.3.0",
        "hafael/easyrec-php": "master"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Hafael\\EasyrecLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/EasyrecLaravelTest.php"
        ],
        "psr-4": {
            "Hafael\\EasyrecLaravel\\": "packages/hafael/easyrec-laravel/src"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "hafael/easyrec-php",
                "version": "master",
                "source": {
                    "type" : "git",
                    "url" : "git://github.com/hafael/easyrec-php.git",
                    "reference" : "master"
                },
                "dist": {
                    "url": "https://github.com/hafael/easyrec-php/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ]
}
